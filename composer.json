{
    "name": "shiyun/php-annotation",
    "description": "php7、8注解,annotation for shiyun ",
    "version": "1.0.1",
    "license": "Apache-2.0",
    "license2": "MulanPSL-2.0",
    "type": "library",
    "authors": [
        {
            "name": "ctocode",
            "email": "343196936@qq.com"
        }
    ],
    "homepage": "https://github.com/10yun/php-annotation",
    "minimum-stability": "dev",
    "minimum-stability2": "stable",
    "require": {
        "php": "^8.0",
        "topthink/framework": "^6.0.12"
    },
    "require-dev": {
        "php": ">=8.0",
        "psr/container": "~1.0",
        "phpunit/phpunit": "^9.5",
        "workerman/webman-framework": ">=1.0",
        "workerman/validation": "^3.0",
        "swagger-api/swagger-ui": "^3.48"
    },
    "autoload": {
        "psr-4": {
            "shiyun\\annotation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests/"
        }
    },
    "extra": {
        "think": {
            "config": {
                "sy-annotation": "src/config/config.php"
            },
            "services": [
                "shiyun\\annotation\\RouteService"
            ]
        }
    }
}